{
    "name": "da/less-bundle",
    "type": "symfony-bundle",
    "description": "Bundle allowing to compile less files",
    "keywords": ["less", "bundle", "da", "compile", "compilation"],
    "homepage": "http://github.com/Gnucki/DaLessBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Prelot",
            "email": "tprelot@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/http-foundation": ">=2.1,<2.3",
        "symfony/framework-bundle": ">=2.1,<2.3",
        "symfony/security-bundle": ">=2.1,<2.3",
        "symfony/twig-bundle": ">=2.1,<2.3",
        "symfony/finder": ">=2.1,<2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Da\\LessBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Da/LessBundle"
}